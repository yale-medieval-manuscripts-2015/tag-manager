<h3>Listing users</h3>


<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Provider</th>
        <th>UID</th>
        <th>Name</th>
        <th></th>
        <th></th>
        <th></th>
    </tr>
    </thead>

    <tbody>

    <% @users.each do |user| %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.provider %></td>
            <td><%= user.uid %></td>
            <td><%= user.name %></td>
            <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
    </tbody>
</table>

<br>

<%= link_to 'Tags', @tags %>